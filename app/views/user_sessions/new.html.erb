<h1> Login
	 
  <%= form_for @user_session, user_sessions_path do |f| %>
  <%= f.error_messages %> </h1>
<div>
  <%= f.label :login %>
  <%= f.text_field :login %></div>
<div>
  <%= f.label :password %>
  <%= f.password_field :password %></div>
<div>
  <%= f.check_box :remember_me %>
  <%= f.label :remember_me%>
</div>
<div>
  <%= f.submit "Login" %>
</div>
<% end %>
